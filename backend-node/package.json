{
  "name": "oc-backend-node",
  "version": "1.0.0",
  "description": "Simple example of an Express backend",
  "scripts": {
    "start": "npm run build && npm run serve",
    "watch": "concurrently --kill-others \"npm run watch-build\" \"npm run watch-serve\"",
    "build": "tsc",
    "watch-build": "tsc -w",
    "serve": "node dist/server.js",
    "watch-serve": "nodemon dist/server.js"
  },
  "author": "Alejandro Pascual",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mysql": "^2.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/mysql": "^2.15.6",
    "@types/node": "^12.7.2",
    "concurrently": "^4.1.2",
    "typescript": "^3.5.3"
  }
}
